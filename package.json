{
  "name": "riflett",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "health": "ts-node scripts/health.ts",
    "scan:secrets": "node scripts/scan-secrets.js",
    "train:intent": "ts-node --compiler-options '{\"module\":\"commonjs\"}' training/intent/trainIntentModel.ts",
    "eval:intent": "ts-node --compiler-options '{\"module\":\"commonjs\"}' evaluation/intent/evaluateIntentModel.ts",
    "test:intent": "ts-node --compiler-options '{\"module\":\"commonjs\"}' tests/intent-runtime.test.ts",
    "active:sync": "ts-node active_learning/syncIntentAudits.ts",
    "active:queue": "ts-node active_learning/buildLabelQueue.ts",
    "active:merge": "ts-node active_learning/mergeLabels.ts",
    "postinstall": "patch-package || true",
    "mcp:install:core": "cd mcp/core && npm install",
    "mcp:install:database": "cd mcp/database && npm install",
    "mcp:install:agent": "cd mcp/agent && npm install",
    "mcp:install:build": "cd mcp/build && npm install",
    "mcp:build:core": "cd mcp/core && npm run build"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@rneui/base": "4.0.0-rc.7",
    "@rneui/themed": "4.0.0-rc.8",
    "@supabase/supabase-js": "^2.74.0",
    "expo": "^54.0.13",
    "expo-build-properties": "~1.0.9",
    "expo-constants": "^18.0.9",
    "expo-file-system": "~19.0.16",
    "expo-sqlite": "~16.0.8",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-safe-area-context": "5.6.1",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-web": "^0.21.0"
  },
  "devDependencies": {
    "@types/node": "^24.6.2",
    "@types/react": "~19.1.0",
    "eslint": "^9.38.0",
    "expo-module-scripts": "^5.0.7",
    "patch-package": "^8.0.0",
    "semgrep": "^0.0.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.2"
  },
  "private": true,
  "overrides": {
    "react-native-safe-area-context": "5.6.1",
    "@rneui/base": "4.0.0-rc.7",
    "@rneui/themed": "4.0.0-rc.8"
  }
}
