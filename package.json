{
  "name": "riflett",
  "version": "1.0.0",
  "main": "apps/mobile/index.ts",
  "scripts": {
    "start": "expo start --config apps/mobile/app.config.ts --entry-file apps/mobile/index.ts --metro-config apps/mobile/metro.config.js",
    "android": "expo run:android --config apps/mobile/app.config.ts",
    "ios": "expo run:ios --config apps/mobile/app.config.ts",
    "web": "expo start --web --config apps/mobile/app.config.ts --entry-file apps/mobile/index.ts --metro-config apps/mobile/metro.config.js",
    "health": "ts-node scripts/health.ts",
    "scan:secrets": "node scripts/scan-secrets.js",
    "db:migrate:dry-run": "supabase db push --dry-run",
    "test:unit": "vitest run --dir tests/unit",
    "test:int": "vitest run --dir tests/integration",
    "db:migrate:up": "npx supabase db migrate up --env-file .env",
    "db:migrate:down": "npx supabase db migrate down --env-file .env",
    "seed": "npx supabase db seed --env-file .env",
    "lint": "eslint apps/mobile",
    "typecheck": "tsc --noEmit",
    "postinstall": "patch-package",
    "test:codex": "TS_NODE_TRANSPILE_ONLY=1 TS_NODE_COMPILER_OPTIONS='{\"module\":\"nodenext\",\"moduleResolution\":\"nodenext\",\"allowImportingTsExtensions\":true}' ts-node scripts/run-tests.ts"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@rneui/base": "4.0.0-rc.7",
    "@rneui/themed": "4.0.0-rc.8",
    "@supabase/supabase-js": "^2.74.0",
    "date-fns-tz": "^3.2.0",
    "expo": "54.0.22",
    "expo-build-properties": "~1.0.9",
    "expo-constants": "~18.0.10",
    "expo-crypto": "^15.0.7",
    "expo-file-system": "~19.0.16",
    "expo-font": "~14.0.9",
    "expo-sqlite": "~16.0.9",
    "expo-status-bar": "~3.0.8",
    "js-yaml": "^4.1.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-safe-area-context": "5.6.1",
    "react-native-type-animation": "^0.4.4",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-web": "^0.21.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.6.2",
    "@types/react": "~19.1.0",
    "@vitest/coverage-v8": "^4.0.1",
    "baseline-browser-mapping": "^2.9.8",
    "eslint": "^9.38.0",
    "expo-module-scripts": "^5.0.7",
    "patch-package": "^8.0.0",
    "semgrep": "^0.0.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.2",
    "vite": "^7.1.11",
    "vitest": "^4.0.1"
  },
  "private": true,
  "overrides": {
    "react-native-safe-area-context": "5.6.1",
    "@rneui/base": "4.0.0-rc.7",
    "@rneui/themed": "4.0.0-rc.8"
  }
}
